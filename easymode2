using System;

class Program
{
    static void Main()
    {
        int[,] array = new int[4, 4];

        for (int i = 0; i < 4; i++)
        {
            for (int j = 0; j < 4; j++)
            {
                array[i, j] = i * j;
                Console.Write(array[i, j] + "\t");
            }
            Console.WriteLine();
        }
    }
}

using System;

class Program
{
    static void Main()
    {
        int[,] array = new int[3, 3];
        int value = 1;

        for (int i = 0; i < 3; i++)
        {
            for (int j = 0; j < 3; j++)
            {
                array[i, j] = value;
                value++;
                Console.Write(array[i, j] + "\t");
            }
            Console.WriteLine();
        }
    }
}
